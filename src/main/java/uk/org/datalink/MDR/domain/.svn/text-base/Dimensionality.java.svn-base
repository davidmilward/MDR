package uk.org.datalink.MDR.domain;

public class Dimensionality {
	
	  private int id;
	  private Boolean coordinates;
	  private int dimensions;
	  private String dimensionname;
   
	  public void setId(int nid){
		  this.id = nid;
	  }
	  private void setCoordinates(Boolean bc){
		  this.coordinates = bc;
	  }
	  private void setDimensions(int ndim){
		  this.dimensions = ndim;
	  }
	  private void setDimensionname(String dimn){
		  this.dimensionname = dimn;
	  }
	  
	  public int getId(){
		  return this.id;
	  }
	  private Boolean getCoordinates(){
		  return this.coordinates;
	  }
	  private int getDimensions(){
		  return this.dimensions;
	  }
	  private String getDimensionname(){
		  return this.dimensionname;
	  }
	
}
