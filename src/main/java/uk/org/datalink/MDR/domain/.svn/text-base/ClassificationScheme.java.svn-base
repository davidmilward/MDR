package uk.org.datalink.MDR.domain;


/**
 * 
 * <p>Thus is a classification scheme to classify data elements in this  </p> 
 * <p>model.</p> 
 * 
 * @author David Milward
 * @version %I%, %G%
 */
public class ClassificationScheme {

	private int id;
	private int schemeType;
	private String description;
	private String name;
	private String notes;
	
	public void setId(int nid){
		this.id = nid;
	}
	public void setSchemeType(int nst){
		this.schemeType = nst;
	}
	public void setDescription(String desc){
		this.description = desc;
	}
	public void setName(String nm){
		this.name = nm;
	}
	public void setNotes(String nt){
		this.notes = nt;
	}
	
	public int getId(){
		return this.id;
	}
	public int getSchemeType(){
		return this.schemeType;
	}
	public String getDescription(){
		return this.description;
	}
	public String getName(){
		return this.name;
	}
	public String getNotes(){
		return this.notes;
	}
}
