package uk.org.datalink.MDR.domain;

public class Artefact {

	private int id;
	private int modelid;
	private int externalid;
	private String artefacturl;
	private String notes;
	
	public void setId(int nid){
		this.id = nid;
	}
	public void setModelid(int mid){
		this.modelid = mid;
	}
	public void setExternalid(int oid){
		this.externalid = oid;
	}
	public void setArtefacturl(String robj){
		this.artefacturl = robj;
	}
	public void setNotes(String nts){
		this.notes = nts;
	}
	
	public int getId(){
		return this.id;
	}
	public int getModelid(){
		return this.modelid;
	}
	public int getExternalid(){
		return this.externalid;
	}
	public String getArtefacturl(){
		return this.artefacturl;
	}
	public String getNotes(){
		return this.notes;
	}
	
}
