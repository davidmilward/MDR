package uk.org.datalink.MDR.domain;



public class Relationship {
 
	  private int id;
	  private int classscheme_id;
	  private int subject_id;
	  private int obj_id;
	  private int type_id;

	  
	  public void setId(int nid){
		  this.id = nid;
	  }
	  public void setClassscheme_id(int clid){
		  this.classscheme_id = clid;
	  }
	  public void setSubject_id(int sid){
		  this.subject_id = sid;
	  }
	  public void setObj_id(int oid){
		  this.obj_id = oid;
	  }
	  public void setType_id(int tid){
		  this.type_id = tid;
	  }
	 
	  
	  
	  public int getId(){
		  return this.id;
	  }
	  public int getClassscheme_id(){
		  return this.classscheme_id;
	  }
	  public int getSubject_id(){
		  return this.subject_id;
	  }
	  public int getObj_id(){
		  return this.obj_id;
	  }
	  public int getType_id(){
		  return this.type_id;
	  }
	
}
