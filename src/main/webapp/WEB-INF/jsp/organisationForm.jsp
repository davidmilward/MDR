<%@ taglib prefix="form" uri="http://www.springframework.org/tags/form" %>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt" %>
<%@ include file="/WEB-INF/jsp/include.jsp" %>
<%@ include file="/WEB-INF/jsp/header.jsp" %>


<html> 
<body>
<h2>Organisation Details</h2>
 
<form:form method="POST" commandName="Organisation">
<form:errors path="*" cssClass="errorblock" element="div"/>
<table>
 

<tr>
      <td align="right" width="20%">Name:</td>
        <td width="20%">
          <form:input path="name"/>
        </td>
</tr>
<tr>
      <td align="right" width="20%">Description:</td>
        <td width="20%">
          <form:input path="description"/>
        </td>
</tr>
<tr>
      <td align="right" width="20%">Contact:</td>
        <td width="20%">
          <form:input path="contactid"/>
        </td>
</tr>
<tr>
      <td align="right" width="20%">Email:</td>
        <td width="20%">
          <form:input path="organisationemail"/>
        </td>
</tr>

<tr>
<td width="20%"><input type="submit" /></td>
</tr>

</table>
</form:form>
 
</body>
</html>
<%@ include file="/WEB-INF/jsp/footer.jsp" %>