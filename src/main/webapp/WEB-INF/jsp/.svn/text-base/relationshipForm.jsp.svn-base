<%@ taglib prefix="form" uri="http://www.springframework.org/tags/form" %>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt" %>
<%@ include file="/WEB-INF/jsp/include.jsp" %>
<%@ include file="/WEB-INF/jsp/header.jsp" %>


<html> 
<body>
<h2>Country Details</h2>
 
<form:form method="POST" commandName="Relationship">
<form:errors path="*" cssClass="errorblock" element="div"/>
<table>
 
<tr>
      <td align="right" width="20%">Classification Scheme Name:</td>
        <td width="20%">
          <form:input path="classificationScheme"/>
        </td>
</tr>
<tr>
      <td align="right" width="20%">Subject ID:</td>
        <td width="20%">
          <form:input path="subject"/>
        </td>
</tr>
<tr>
      <td align="right" width="20%">Object ID:</td>
        <td width="20%">
          <form:input path="object"/>
        </td>
</tr>

<tr>
      <td align="right" width="20%">Type ID:</td>
        <td width="20%">
          <form:input path="type"/>
        </td>
</tr>

<tr>
<td colspan="3"><input type="submit" /></td>
</tr>

</table>
</form:form>
 
</body>
</html>
<%@ include file="/WEB-INF/jsp/footer.jsp" %>