/*main elements*/
BODY, DIV, TD
{
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	color: #666;
}
BODY
{
	background-color: #FFF;
	background-image: url(../images/banner-graphic.png);
	background-position: top center;
	background-repeat: no-repeat;
	text-align: center;
	min-width: 600px;
	margin-top: 60px;
	margin-left: auto;
	margin-right: auto;
}
DIV
{
	margin: 5px 25px;
	text-align: center;
}
/*header and footer elements*/
#main
{
	margin: 0 auto;
	position: relative;
	top: 35px;
	left: 0px;
	width: 560px;
	text-align: left;
}
.footer
{
	background-image: url(#97);
	border: none;
	margin-top: 20px;
	border-top: 1px solid #999999;
	width: 100%;
	background-color: #070978;
	color: #FFFFFF;
	
}
.footer TD
{
	color: #999999;
}
.footer A:link
{
	color: #7DB223;
}
/*text styles*/
H1, H2, H3
{
	font-family: Helvetica, sans-serif;
	color: #120AAA;
}
H1
{
	font-size: 20px;
	line-height: 26px;
}
H2
{
	font-size: 18px;
	line-height: 24px;
}
H3
{
	font-size: 15px;
	line-height: 21px;
	color: #555;
}
H4
{
	font-size: 14px;
	line-height: 20px;
}
.errors
{
	color: #000000;
}
A
{
	text-decoration: underline;
	font-size: 13px;
}
A:link
{
	color: #3671D4;
}
A:hover
{
	color: #6220E9;
}
A:active
{
	color: #3CA6F3;
}
A:visited
{
	color: #2A2C80;
}
UL
{
	list-style: disc url(../images/bullet-arrow.png);
}
LI
{
	padding-top: 5px;
	text-align: left;
}
LI UL
{
	list-style: square url(images/sub-bullet.gif);
}
LI UL LI UL
{
	list-style: circle none;
}
