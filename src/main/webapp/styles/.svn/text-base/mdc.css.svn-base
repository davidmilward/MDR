TABLE
{
	background: #FFFF33;
	margin: 3px 0 0;
	border: 4px solid blue;
	border-collapse: collapse;
	align: right

}
TABLE TABLE
{
	margin: -5px 0;
	border: 1px solid #FFFF33;
	width: 100%;
	background-color: #FFFF33;
	align:center
	width:80%
}
TABLE TD, TABLE TH
{
	padding: 8px;
	background-color:#CCD7EA;
	color:#333399;
	align:right 
}
TABLE TH
{
	font-size: 12px;
	text-align: right;
	font-weight: bold;
}
TABLE THEAD
{
	font-weight: bold;
	font-style: italic;
	background-color: #CCD7EA;
}
TABLE A:link
{
	color: #303030;
}
CAPTION
{
	caption-side: top;
	width: auto;
	text-align: center;
	font-size: 12px;
	color: #3399CC;
	padding-bottom: 4px;
  background: #0000CC
}
FIELDSET
{
	background: #E0E7D3;
	padding: 8px;
	padding-bottom: 22px;
	border: none;
	width: 560px;
}
FIELDSET LABEL
{
	width: 70px;
	float: center;
	margin-top: 1.7em;
	margin-left: 20px;
}
/*fieldset textfield*/
#INVALID-RULE
{
	margin: 3px;
	height: 20px;
	background: #E0E7D3;
}
FIELDSET TEXTAREA
{
	margin: 3px;
	height: 165px;
	background: #E0E7D3;
}
FIELDSET INPUT
{
	margin: 3px;
	height: 20px;
	background: #E0E7D3;
}
FIELDSET TABLE
{
	width: 100%;
}
FIELDSET TH
{
	padding-left: 25px;
}

table.center {
    margin-left:auto; 
    margin-right:auto;
  }
.table-buttons
{
	background-color: #FFF;
	border: none;
}
.table-buttons TD
{
	border: none;
}
.submit INPUT
{
	background: url(../images/submit-bg.png) repeat-x;
	border: 2px outset #D7B9C9;
	color: #383838;
	padding: 2px 10px;
	font-size: 11px;
	text-transform: uppercase;
	font-weight: bold;
}
.updated
{
	background: #ECF1E5;
	font-size: 11px;
	margin-left: 2px;
	border: 4px solid #ECF1E5;
}
.updated TD
{
	padding: 2px 8px;
	font-size: 11px;
	color: #888888;
}
